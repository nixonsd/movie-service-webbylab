# .env.example for movie-service-webbylab

# SQLite database file path.
# If unset: uses ':memory:' in test, otherwise 'data/database.sqlite'
SQLITE_FILE=

# Node environment: development | production | test
NODE_ENV=development

# API server port
PORT=8000

# Access token secret (replace with a strong secret)
ACCESS_TOKEN_SECRET=your-strong-secret-here